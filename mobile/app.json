{
    "expo": {
        "name": "app-name",
        "slug": "app-name",
        "version": "0.0.0",
        "orientation": "portrait",
        "icon": "./assets/icons/ic_app.png",
        "scheme": "app-name",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/images/img_splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/icons/ic_app.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.chuazz.appname"
        },
        "plugins": [],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {
                "origin": false
            }
        },
        "owner": "chuazz"
    }
}
