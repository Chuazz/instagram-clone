{
    "expo": {
        "name": "instagram-clone",
        "slug": "instagram-clone",
        "version": "0.0.0",
        "orientation": "portrait",
        "icon": "./assets/icons/ic_app.png",
        "scheme": "instagram-clone",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/images/img_splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/icons/ic_app.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.chuazz.instagramClone"
        },
        "plugins": [
            [
                "expo-image-picker",
                {
                    "photosPermission": "The app accesses your photos to let you share them with your friends."
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {
                "origin": false
            }
        },
        "owner": "chuazz"
    }
}
