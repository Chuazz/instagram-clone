{
	"$schema": "https://turbo.build/schema.json",
	"globalDependencies": [".env"],
	"tasks": {
		"build": {
			"inputs": ["$TURBO_DEFAULT$", ".env*", ".env.local"],
			"outputs": ["dist/**", ".next/**", "!.next/cache/**"],
			"dependsOn": ["^build"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"lint": {},
		"clean": {
			"cache": false
		}
	}
}
