{
	"name": "@instagram/types",
	"version": "0.0.0",
	"private": true,
	"main": "./dist/index.js",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"default": "./dist/index.js"
		},
		"./data": {
			"types": "./src/data/index.ts",
			"default": "./dist/data/index.js"
		}
	},
	"scripts": {
		"dev": "tsc --watch",
		"dev:mobile": "yarn dev",
		"dev:package": "yarn dev",
		"build": "tsc",
		"build:package": "yarn build"
	},
	"dependencies": {
		"@instagram/typescript-config": "*"
	}
}