{
	"expo": {
		"name": "instagram-clone",
		"slug": "instagram-clone",
		"version": "0.0.0",
		"orientation": "portrait",
		"icon": "./assets/icons/ic_app.png",
		"scheme": "instagram-clone",
		"assetBundlePatterns": ["**/*"],
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./assets/images/img_splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/icons/ic_app.png",
				"backgroundColor": "#ffffff"
			},
			"package": "com.chuazz.instagramClone"
		},
		"plugins": [
			[
				"expo-media-library",
				{
					"photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
					"savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
					"isAccessMediaLocationEnabled": true
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {
				"origin": false
			}
		},
		"owner": "chuazz"
	}
}
